	<script>
		$(function(e){
			var faqList=$("div.box>ul>li");
			faqList.attr("class","off");
			

			function onFaqList(){
				if($(this).parents("li").hasClass("on")){
					$(this).next("dd").stop().animate({opacity:"0"},"fast").parents("li").removeClass("on").addClass("off");
					
				}else{
					faqList.attr("class","off").find("dd").css("opacity","0");
					$(this).next("dd").stop().animate({opacity:"1"},"fast").parents("li").removeClass("off").addClass("on");

				}
			}
			faqList.find("dt").click(onFaqList);

			
			

		$("div.ham_bar").click(function(){
				$("nav.open").css("display","block");
				$("nav.open").find("div.nav_con").stop().animate({left:"20%"});
				$("div.bg").stop().animate({opacity:"1"});
				$("#nav_menu").css("display","none");
			return false;
			
			});
		
			$("div.btn_off").click(function(){
				$("nav.open").css("display","none");
				$("div.nav_con").stop().animate({left:"100%"});
				$("div.bg").stop().animate({opacity:"0"});
				$("#nav_menu").css("display","block");
				
			return false;

			});

			
		 $(window).resize(function(){
				$(".open").css("display","none");
				$("#nav_menu").css("display","block");
			});
		});	

 
		
	</script>

