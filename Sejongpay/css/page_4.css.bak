															background-size: 8px 16px; }
		
		div.nav_con  > ul  > li > ul.hidden_w {width:100%;  float:none;}
		div.nav_con ul.hidden_w > li{float:none;}
		div.nav_con ul.hidden_w a{display: block; width: 100%;   height: 2.5rem;    line-height: 2.5rem;    padding-left: 2rem;    font-size: 1rem !important;    color: #555555 !important;  
														border-bottom: 1px solid #ccc;																
															background: #dedede;}	
		div.nav_con ul.hidden_w a:hover{color:#e20613 !important;}
		div.nav_con ul li > ul > li > a:visited{ color: #333333;}

		div.nav_con ul > li:nth-child(1) > a.first{color:#e20613 !important;background:#f2f2f2 url(../img/bl_menu_on.png)88% 55% no-repeat !important; background-size: 8px 16px !important;}
		

		div.nav_con div.util_con{position: absolute;    bottom: 0px;    width: 100%;    height:50px; border-top: 1px solid #ccc;  background: #f2f2f2;    line-height: 2.25rem; }
		div.nav_con div.util_con a{display:inline-block; }
		div.nav_con div.util_con a:nth-child(1){height: 2rem;  line-height: 2rem; margin-top:0.5rem;  padding-right: 1rem;    padding-left: 2rem !important;  margin-left: 1rem;background: #00c73c url(../img/icon_naverpost_m.png)8% 60% no-repeat;     
																			background-size: 11px 18px!important;  color: #fff !important; font-size:14px !important;     }
		div.nav_con div.util_con a:nth-child(2){font-size: 1.125rem !important;    padding-left: 0.5rem !important;    height: 3rem;    line-height: 3rem;background: none !important;}



			section{margin:100px auto;}
			section h2 + strong{font-size:16px; color:#222;}
		
		    footer div.uni_con{background:none; border:none; padding: 0.2rem 1rem;}
			footer div.footer_con {text-align:center; }
			footer div.footer_con span{padding: 0 1rem;}
			footer div.footer_con a{padding-right:0.8rem;}
			div.address_con{text-align:center; padding: 0 1rem 1.25rem; line-height: 1.75rem;margin: 0 auto; padding-right: 5rem;    max-width: 1220px;}
			div.address_con > img {float:none;   padding-bottom: 1rem; margin:-50px 0 0 0; padding-right: 3rem;}
			div.address_con > p:nth-child(2){padding-top:0px; font-size:14px; color:#777777;}
			div.address_con > p:nth-child(3){font-size:12px; color:#ccc; margin-top:0px;}
			a.btn_footer{top: 0.8rem;right: auto; left: 50%; width: 31px;height: 44px;margin-left: 4.5rem;background-size: 31px 44px;}
			a.btn_download{display:none;}

		
	}
 
 
	
