@charset "utf-8";

@import url(http://fonts.googleapis.com/earlyaccess/notosanskr.css);

 /*초기화 영역*/
 body, header, section, footer, div, p, ul, li, dl, dd, dt, h1,h2,h3,h4,h5,h6{margin:0; padding:0;}
	img{border:none;}
	a{text-decoration:none; color:#000;}
	ul{list-style:none;}
	em{color:#e20613; font-style:normal;}
	
	
	body{font-family: 'Noto Sans KR', sans-serif; background:#f6f6f6;}

/*헤더 영역*/ 
	header{width:100%; height:120px; background:#fff; position:fixed; z-index:99; top:0;}
	header ul{float:right;}
	header ul > li{float:left;}

	/*유틸메뉴*/
	#top_menu{width:100%;  height:35px;  border-bottom:1px solid #ccc;}
			#util_menu{max-width:1220px;height:35px; margin:0 auto;  padding:0 12px;}
			#util_menu > ul{height:100%;font-size:13px; position:relative; top:8px; }
			#util_menu > ul > li{height:100%;float:left; text-indent:24px;  margin-right:15px; letter-spacing:-1px; position:relative; font-size:13px;  }
			#util_menu > ul > li > a{display:block;  width:100%; height:100%; line-height:20px;}
			#util_menu > ul > li:nth-child(1) a {background:url(../images/icon_naverpost.png)no-repeat; background-size:21px;}
			#util_menu > ul > li:nth-child(1){text-indent:28px; }
			#util_menu > ul > li:nth-child(2) a{background:url(../images/icon_faq_b.png)no-repeat;}
			#util_menu > ul > li:nth-child(3){margin-right:0px; text-indent:24px;}
			#util_menu > ul > li:nth-child(3) a{color:#e20613;background:url(../images/icon_admin.png)no-repeat; }
	
			/*로고 및 나브*/
			#nav_menu{max-width:1220px; height:80px; margin:0 auto; position:relative;  top:0px; padding:0 12px;}
			#nav_menu > h1#logo{width:12.5rem; height:3rem;	position:relative; top:18px; }
			 h1#logo > a{width:12.5rem; height:3rem; display:block; text-indent:-9999px;background:url(../images/logo_b.png)no-repeat; 
						background-size:12.5rem 3rem;   position:relative; }
			nav.main_menu  > ul { position:relative; top:-15px; }
			nav.main_menu  > ul > li{ margin-right:8px; font-size:18px;text-align:right; margin-right:35px;}
			nav.main_menu  > ul > li:hover a{color:#e20613;}
			nav.main_menu  > ul > li:nth-child(1){margin-left:180px;}
				nav.main_menu  > ul > li:nth-child(6){margin-right:0px;}
			nav.main_menu > ul > li > a{display:block; width:100%; height:100%;  }

			div.ham_bar{position:absolute; height:24px; width:24px; margin-top:-26px; right:30px; cursor:pointer; display:none;}
			div.ham_bar span{position:absolute; width:100%; height:2px;background-color:#000;}
			div.ham_bar span:nth-child(2){margin-top:8px;}
			div.ham_bar span:nth-child(3){margin-top:16px;}
		
			nav.open{display:none;}
			
	

/*섹션 영역*/ 
section{max-width:1600px; height:100%; margin:130px auto;}
	section > h2{text-align:center; margin:20px 0;  padding-top:33px; font-weight:400; font-size:38px;}
	section > strong{display:block; text-align:center; font-size:20px;font-weight:300; padding:0 40px 40px 40px;}
		
		/*서치바*/
		div#search_bar{max-width:1180px; height: 1.625rem;  padding:0 18px; margin:0 auto;  margin-bottom: 2.5rem;   }
			div.page_info{float:left;  font-size:15px; }
			div.search{float:right;}
				div.search input{height: 2.625rem;padding: 0 2rem;   width: 200px;border: 1px solid #d2d2d2; background: #fff;   padding: 0 0rem 0 1rem;}
				div.search input + a{display:inline-block;height: 2.625rem;padding: 0 2rem;background: #666666; color:#fff;  line-height: 2.625rem; font-size:14px;
									vertical-align: top;}
		
		/*콘텐츠*/
		div#content{max-width:1220px;margin-bottom:0px; height:100%; margin:0 auto; overflow:hidden;}
			
			div.box{padding:0 18px;}
			
			div.box li{margin-bottom:10px; font-weight:300; }	
			div.box dl{background:#fff; padding:0.6px;  }
			div.box li dl dt{position: relative; padding: 2rem 2rem 2rem 4rem;font-size: 1.375rem;    
									background:url(../images/faq_off.png)95% 50% no-repeat; cursor: pointer; text-indent:25px; }
			div.box dl dt span.q{position:absolute; top:15px; left: 3%; font-size: 2.875rem;  color: #fa2829;   margin-right: 1rem; }
			div.box dl dt span.text{position:relative; line-height:18px; margin-left:3%;}
			div.box dl dd{color:#777777; height: 100%; padding: 1rem 0rem 2rem 4.5rem; line-height:25px; border-top: 1px solid #e0e0e0;position: relative; margin:0 50px;}
			div.box li.on {border:1px solid #e20613; }
			div.box li.on dl dt{background:url(../images/faq_on.png)95% 50% no-repeat; }
			div.box li.off dd{display:none;}
			
			div.list_page{text-align: center; margin-top:80px; }
			div.list_page span{display:inline-block; width: 1.875rem; height: 1.875rem; cursor:pointer; 
											line-height: 1.875rem; margin:0 0.2rem; }
			div.list_page span a{display:block; }
			div.list_page span.btn_on a{background: #e20613; color: #fff; }

	/* 푸터 영역*/
	footer{background:#fff; clear:both;width:100%;  height:100%;}
	footer div.uni_con{width:100%; border-bottom:1px solid #ccc;}

	footer div.footer_con{max-width:1220px; height:40px; padding:20px 20px 10px 20px; margin:0 auto;}
	footer div.footer_con a{display:inline-block; margin-top:10px; font-size: 13px;}
	
	footer div.address_con{max-width:1220px; margin:0 auto;  padding:20px 20px 30px 20px; position:relative;}
	div.address_con > img{float:left; padding-top:15px; margin:0 30px 0 0;height:100%;}
	div.address_con > p:nth-child(2){padding-top:15px; font-size:14px; color:#777777;}
	div.address_con > p:nth-child(3){font-size:12px; color:#acacac;; margin-top:5px;}
	a.btn_footer{width:44px; height:62px; background:url(../images/icon_wa.png)no-repeat;position:absolute; text-indent:-9999px;
				cursor:pointer; top:30px; right:298px;}
	a.btn_download{width:265px; height:38px; background:url(../images/icon_down.png) no-repeat 10px center; position:absolute; background-color:#2755ac; 
									padding-top:15px; margin-right:15px;color:#fff; text-align:center; font-size: 17px; right:0px; top:30px; cursor:pointer;
									text-indent:12px;}




/*미디어 영억*/

 
